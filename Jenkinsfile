pipeline {
    agent any

    stages {
        stage('Hello') {        //Stage Name
            steps {
                echo 'Hello World'       //Print / Msgbox
            }
        }
        stage('Build') {        //Stage Name
            steps {
                echo 'Buildin'   //Print / Msgbox
            }
        }
        stage('Deploy') {        //Stage Name
            steps {
                echo 'Deploying'   //Print / Msgbox
            }
        }
        stage('Smoke Test') {        //Stage Name
            steps {
                echo 'Testing'   //Print / Msgbox
            }
        }
    }
}
